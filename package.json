{
  "name": "sam_cli",
  "version": "1.0.1-beta.8",
  "main": "index.js",
  "repository": "https://crycime@github.com/crycime/sam_cli.git",
  "author": "zizhenli <zizhenli@hk01.com>",
  "license": "MIT",
  "bin": {
    "sam": "./index.js"
  },
  "scripts": {
    "test": "jest",
    "bumpVersion:master": "standard-version --commit-all --message 'chore(production): %s\n\n[skip master-ci]' --no-verify",
    "bumpVersion:release": "standard-version --commit-all --message 'chore(release): %s\n\n[skip release-ci]' --no-verify --prerelease beta",
    "bumpVersion:master:dry": "standard-version --commit-all --message 'chore(production): %s\n\n[skip master-ci]' --no-verify --dry-run",
    "bumpVersion:release:dry": "standard-version --commit-all --message 'chore(release): %s\n\n[skip release-ci]' --no-verify --prerelease beta --dry-run"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "standard-version": "^8.0.1"
  }
}
